#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    show_song_table_header: false,
    draw_borders: true,
    browser_column_widths: [20, 38, 42],
    background_color: None,
    modal_backdrop: true,
    text_color: None,
    header_background_color: None,
    modal_background_color: None,
    preview_label_style: (fg: "yellow"),
    preview_metadata_group_style: (fg: "yellow", modifiers: "Bold"),
    song_table_album_separator: None,
    tab_bar: (
        enabled: true,
        active_style: (fg: "black", bg: "blue", modifiers: "Bold"),
        inactive_style: (),
    ),
    highlighted_item_style: (fg: "green", modifiers: "Bold"),
    current_item_style: (fg: "black", bg: "blue"),
    borders_style: (fg: "blue", modifiers: "Bold"),
    highlight_border_style: (fg: "blue"),
    symbols: (song: "󰝚 ", dir: " ", playlist: "󰲸 ", marker: "@ ", ellipsis: "...",
        song_style: (fg: "blue"), dir_style: (fg: "red")
    ),
    progress_bar: (
        symbols: ["", "█", "", "█", "" ],
        track_style: (fg: "black"),
        elapsed_style: (fg: "green", bg: "green"),
        thumb_style: (fg: "green", bg: "black"),
    ),
    scrollbar: (
        symbols: ["│", "█", "▲", "▼"],
        track_style: (fg: "white"),
        ends_style: (fg: "white"),
        thumb_style: (fg: "blue"),
    ),
    song_table_format: [
        (
        prop: (kind: Transform(Replace(content: (kind: Sticker("rating")), replacements: [
            (match:  "0", replace: (kind: Group([(kind: Text("󰎡"))]))),
            (match:  "1", replace: (kind: Group([(kind: Text("󰎤"),style: (fg: "blue"))]))),
            (match:  "2", replace: (kind: Group([(kind: Text("󰎧"),style: (fg: "blue"))]))),
            (match:  "3", replace: (kind: Group([(kind: Text("󰎪"),style: (fg: "blue"))]))),
            (match:  "4", replace: (kind: Group([(kind: Text("󰎭"),style: (fg: "blue"))]))),
            (match:  "5", replace: (kind: Group([(kind: Text("󰎱"),style: (fg: "blue"))]))),
            (match:  "6", replace: (kind: Group([(kind: Text("󰎳"),style: (fg: "blue"))]))),
            (match:  "7", replace: (kind: Group([(kind: Text("󰎶"),style: (fg: "blue"))]))),
            (match:  "8", replace: (kind: Group([(kind: Text("󰎹"),style: (fg: "blue"))]))),
            (match:  "9", replace: (kind: Group([(kind: Text("󰎼"),style: (fg: "blue"))]))),
            (match: "10", replace: (kind: Group([(kind: Text("󰽽"),style: (fg: "blue"))]))),
        ])), default: (kind: Text(""), style: (fg: "yellow"))),
        width: "3",
        label: "Rating",
        alignment: Left,
        ),
        (
            prop: (kind: Property(Artist), style: (fg: "red"),
                default: (kind: Text("Unknown Artist"), style: (fg: "black"))),
            width: "21%"
        ),
        (
            prop: (kind: Property(Title),
                default: (kind: Property(Filename))),
            width: "45%"
        ),
        (
            prop: (kind: Property(Album), style: (fg: "red"),
                default: (kind: Text("Unknown Album"), style: (fg: "black"))),
            width: "34%"
        ),
        (
            prop: (kind: Property(Duration), style: (fg: "green"),
                default: (kind: Text("-"))),
            width: "5%", alignment: Right
        ),
    ],
    layout: Split(
        direction: Vertical, panes: [
            (size: "3", pane: Pane(Tabs)),
            (size: "4", borders: "ALL", pane: Component("custom-header")),
            (size: "100%", borders: "NONE", pane: Pane(TabContent)),
            (size: "3", borders: "TOP | BOTTOM", pane: Pane(ProgressBar)),
        ]),
    header: (rows: []),
    browser_song_format: [
        (kind: Group([
                    (kind: Property(Track)),
                    (kind: Text(" ")),
                ])),
        (kind: Group([
                    (kind: Property(Artist)),
                    (kind: Text(" - ")),
                    (kind: Property(Title)),
                ]), default: (kind: Property(Filename)))
    ],
    level_styles: (
        info:  (fg: "yellow", bg: "black"),
        warn:  (fg: "blue", bg: "black"),
        error: (fg: "blue", bg: "black"),
        debug: (fg: "blue", bg: "black"),
        trace: (fg: "blue", bg: "black"),
    ),
    components: {
        "custom_song_table_header": Split(borders: "ALL", direction: Horizontal, panes: [
                (size: "5", pane: Pane(Property(content: [
                                (kind: Text(" 󰩳"), style: (fg: "blue")),
                            ], align: Left))
                ),
                (size: "21%", pane: Pane(Property(content: [
                                (kind: Text("Artist "), style: (fg: "white")),
                                (kind: Text(""), style: (fg: "blue"))
                            ], align: Left))
                ),
                (size: "39%", pane: Pane(Property(content: [
                                (kind: Text("Title "), style: (fg: "white")),
                                (kind: Text(""), style: (fg: "blue"))
                            ], align: Left))
                ),
                (size: "33%", pane: Pane(Property(content: [
                                (kind: Text("Album "), style: (fg: "white")),
                                (kind: Text("󰀥"), style: (fg: "blue"))
                            ], align: Left))
                ),
                (size: "7%", pane: Pane(Property(content: [
                                (kind: Text("Length "), style: (fg: "white")),
                                (kind: Text(" "), style: (fg: "blue"))
                            ], align: Right))
                ),
                (size: "1", pane: Pane(Property(content: [
                                (kind: Text(" "), style: (fg: "blue")),
                            ], align: Left))
                ),
            ]),
        "custom-header": Split(direction: Horizontal, panes: [
                (size: "33%", pane: Split(direction: Vertical, panes: [
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Text(""), style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(StateV2(playing_label: "  ", paused_label: "  ", stopped_label: "  "))),style: (fg: "white")),
                                            (kind: Text(" "), style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Widget(ScanStatus)), style: (fg: "white", modifiers: "Bold"))
                                        ], align: Left))
                            ),
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Text("[ "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(Elapsed)),style: (fg: "white")),
                                            (kind: Text(" / "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(Duration)),style: (fg: "white")),
                                            (kind: Text(" | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(Bitrate)),default: (kind: Text(" ")),style: (fg: "white")),
                                            (kind: Text(" kbps"),style: (fg: "blue")),
                                            (kind: Text(" ]"),style: (fg: "blue", modifiers: "Bold"))
                                        ], align: Left))
                            ),
                        ])
                ),
                (size: "34%", pane: Split(direction: Vertical, panes: [
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Property(Song(Title)), style: (fg: "white",modifiers: "Bold"),
                                                default: (kind: Property(Song(Filename)), style: (fg: "white",modifiers: "Bold")))
                                        ], align: Center))
                            ),
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Property(Song(Artist)), style: (fg: "green"),
                                                default: (kind: Text("Unknown Artist"), style: (fg: "green"))),
                                            (kind: Text(" - "), style: (fg: "white")),
                                            (kind: Property(Song(Album)),style: (fg: "blue" ),
                                                default: (kind: Text("Unknown Album"), style: (fg: "blue")))
                                        ], align: Center))
                            ),
                        ])
                ),
                (size: "33%", pane: Split(direction: Vertical, panes: [
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Text(" 󱡬"), style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(Volume)), style: (fg: "white")),
                                            (kind: Text("% "), style: (fg: "blue", modifiers: "Bold"))
                                        ], align: Right))
                            ),
                            (size: "100%", pane: Pane(Property(content: [
                                            (kind: Text("[ "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(QueueLength())),style: (fg: "white")),
                                            (kind: Text(" 󰴍 | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(QueueTimeTotal(separator: " "))),style: (fg: "white")),
                                            (kind: Text(" 󱎫 | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(RepeatV2(on_label: "", off_label: "",
                                                            on_style: (fg: "blue", modifiers: "Bold"), off_style: (fg: "black", modifiers: "Bold"))))),
                                            (kind: Text(" | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(RandomV2(on_label: "", off_label: "",
                                                            on_style: (fg: "blue", modifiers: "Bold"), off_style: (fg: "black", modifiers: "Bold"))))),
                                            (kind: Text(" | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(ConsumeV2(on_label: "󰮯", off_label: "󰮯", oneshot_label: "󰮯󰇊",
                                                            on_style: (fg: "blue", modifiers: "Bold"), off_style: (fg: "black", modifiers: "Bold"))))),
                                            (kind: Text(" | "),style: (fg: "blue", modifiers: "Bold")),
                                            (kind: Property(Status(SingleV2(on_label: "󰎤", off_label: "󰎦", oneshot_label: "󰇊", off_oneshot_label: "󱅊",
                                                            on_style: (fg: "blue", modifiers: "Bold"), off_style: (fg: "black", modifiers: "Bold"))))),
                                            (kind: Text(" ]"),style: (fg: "blue", modifiers: "Bold")),
                                        ], align: Right))
                            ),
                        ])
                ),
            ])
    },
    cava: (
        bar_symbols: ['▁', '▂', '▃', '▄', '▅', '▆', '▇', '█'],
        bar_width: 1, bar_spacing: 1,
        bar_color: Rows([
            "yellow",
            "yellow",
            "green",
            "green",
        ])
        )
    ),
)
